<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="shipment_items">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Wismolabs_Tracking::shipping/items.phtml</argument>
            </action>
            <arguments>
                <argument name="shippingItemsViewModel" xsi:type="object">Wismolabs\Tracking\ViewModel\ShippingItemsViewModel</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="sales.order.info.buttons">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Wismolabs_Tracking::order/info/buttons.phtml</argument>
            </action>
            <arguments>
                <argument name="infoButtonsViewModel" xsi:type="object">Wismolabs\Tracking\ViewModel\InfoButtonsViewModel</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
